<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (var dict in ViewBag.jobs)
{

    <table class="job-listing">
        <tbody>
            @foreach (var row in dict)
            {

                <tr>
                    <th>@row.Key</th>
                    <td>@row.Value</td>
                </tr>
            }
        </tbody>
    </table>

}